{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "postinstall": "cd node_modules/@beehiveinnovation/rain-protocol && npm install --ignore-scripts && npm run build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "rain-sdk-ts",
  "author": "Josh Hardy",
  "module": "dist/rain-sdk-ts.esm.js",
  "size-limit": [
    {
      "path": "dist/rain-sdk-ts.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rain-sdk-ts.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.4",
    "husky": "^7.0.4",
    "size-limit": "^6.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@beehiveinnovation/rain-protocol": "beehive-innovation/rain-protocol.git#9236037157ebe468b491cacf14cd3e783080d97a",
    "@uniswap/sdk-core": "^3.0.1",
    "async": "^3.2.2",
    "ethers": "^5.5.1",
    "standardx": "3.0.0"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^5.4.0",
    "**/@typescript-eslint/parser": "^5.4.0",
    "**/jest": "^27.3.1",
    "**/ts-jest": "^27.0.7",
    "**/typescript": "^4.4.4"
  }
}
